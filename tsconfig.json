{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "angular-components": [
        "dist/angular-components"
      ],
      "angular-components/*": [
        "dist/angular-components/*"
      ],
      "footer-component": [
        "dist/footer-component"
      ],
      "footer-component/*": [
        "dist/footer-component/*"
      ],
      "footer": [
        "dist/footer"
      ],
      "footer/*": [
        "dist/footer/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}